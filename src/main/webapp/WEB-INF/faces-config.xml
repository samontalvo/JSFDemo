<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">

	<!-- Configuración de Beans Administrados -->
	<managed-bean>
		<managed-bean-name>pruebaBean</managed-bean-name>
		<managed-bean-class>com.mitocode.mbean.PruebaBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>personaBean</managed-bean-name>
		<managed-bean-class>com.mitocode.mbean.PersonaBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<!-- Otros elementos de configuración -->
	<!-- Puedes agregar otros elementos según las necesidades de tu aplicación -->

</faces-config>